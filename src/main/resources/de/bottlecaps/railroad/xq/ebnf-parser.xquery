          let $state := p:lookahead3W(43, $input, $state)   (: Whitespace | NCName | StringLiteral | CharCode |
                                                               WsExplicit | WsDefinition | DocComment | EOF |
                                                               EquivalenceLookAhead | '$' | '&' | '(' | ')' | '**' |
                                                               '++' | '-' | '.' | '/' | '::=' | '<?' | '<?ENCORE?>' |
                                                               '<?TOKENS?>' | '[' | '[^' | '|' :)
          return $state
