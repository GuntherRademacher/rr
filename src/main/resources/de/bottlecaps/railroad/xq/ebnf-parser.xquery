        else if ($state[$p:l1] = 23) then                   (: '**' :)
          let $state := p:consume(23, $input, $state)       (: '**' :)
          let $state := p:lookahead1W(26, $input, $state)   (: Whitespace | NCName | StringLiteral | CharCode | '$' |
