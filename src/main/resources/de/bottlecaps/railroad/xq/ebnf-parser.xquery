                                                               '**' | '+' | '++' | '-' | '.' | '/' | '::=' | '<<' |
                                                               '<?' | '<?ENCORE?>' | '<?TOKENS?>' | '>>' | '?' | '[' |
                                                               '[^' | '\\' | '|' :)
      let $state :=
        if ($state[$p:lk] eq 259) then                      (: NCName Context :)
          let $state := p:lookahead3W(48, $input, $state)   (: Whitespace | NCName | StringLiteral | CharCode |
                                                               WsExplicit | WsDefinition | DocComment | EOF |
                                                               EquivalenceLookAhead | '$' | '&' | '(' | ')' | '*' |
                                                               '**' | '+' | '++' | '-' | '.' | '/' | '<<' | '<?' |
                                                               '<?ENCORE?>' | '<?TOKENS?>' | '>>' | '?' | '[' | '[^' |
